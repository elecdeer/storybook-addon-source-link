{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"incremental": false,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"lib": ["es2020", "dom"],
		"module": "preserve",
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"moduleResolution": "bundler",
		"noImplicitAny": true,
		"skipLibCheck": true,
		"target": "ES2020",
		"strict": true
	}
}

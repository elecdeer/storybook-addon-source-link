{
	"compilerOptions": {
		"rootDir": ".",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"incremental": false,
		"isolatedModules": true,
		"jsx": "react",
		"lib": ["esnext", "dom", "dom.iterable"],
		"module": "preserve",
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"moduleResolution": "bundler",
		"noImplicitAny": true,
		"skipLibCheck": true,
		"target": "esnext",
		"strict": true
	}
}
